<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.8.9">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[psr-form1]]></title>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[Submit]]></text>
		</button>
		<fields>
			<field id="1" size="medium" type="text">
				<label><![CDATA[name]]></label>
			</field>
			<field id="2" size="medium" type="number" numberFormat="decimal_dot">
				<label><![CDATA[no]]></label>
			</field>
			<field id="3" size="medium" type="text">
				<label><![CDATA[email]]></label>
			</field>
			<field id="4" size="medium" type="text">
				<label><![CDATA[notes]]></label>
			</field>
			<field id="5" size="medium" type="text">
				<label><![CDATA[follows]]></label>
			</field>
			<field id="6" type="text" size="medium">
				<label><![CDATA[greetings]]></label>
			</field>
		</fields>
		<confirmations>
			<confirmation id="56c6f7f44af4b" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
			</confirmation>
		</confirmations>
		<notifications>
			<notification id="56c6f7f44ab2c">
				<to><![CDATA[{admin_email}]]></to>
				<name><![CDATA[Admin Notification]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[email]]></toType>
				<subject><![CDATA[New submission from {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
			</notification>
		</notifications>
	</form>
</forms>